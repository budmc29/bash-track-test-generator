#!/usr/bin/env bats

<% for test in @test_map do -%>
<%# set input so we can access its values -%>
<% input = test["input"] -%>
<%# get the test header -%>
@test "<%= test["description"] %>" {
  skip
  <%= @run_string %> <%= input["number"] %>
<% if Hash === test["expected"] -%>
  [ "$status" -eq 1 ]
  [ "$output" == "Error: <%= test["expected"]["error"] %>" ]
<% else -%>
  [ "$status" -eq 0 ]
  [ "$output" -eq <%= test["expected"] %> ]
<% end -%>
}

<% end -%>
